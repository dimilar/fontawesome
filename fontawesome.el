;; Example 1: Insert all fonts at the cursor position
(defun font-awesome-insert-all ()
  (interactive)
  (mapc (lambda (font)
          (insert " ")
          (ov-set (ov-insert (car font)) 'face '(:family "FontAwesome" :height 1.5)))
        font-awesome-list))

(defun font-awesome-insert()
  (interactive)
  (let (font-name char ov)
    (setq prompt "Choose icon: ")
    (setq input (completing-read prompt font-awesome-list nil t))
    (setq font-name (assoc input font-awesome-list))
    (setq ov (ov-insert (car font-name)))
    (ov-set ov 'face '(:family "FontAwesome"))))

(provide 'fontawesome)
